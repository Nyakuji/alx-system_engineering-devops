#!/usr/bin/env bash
# This script displays different messages for loop iterations.

# Initialize the counter
count=1

# The while loop to iterate from 1 to 10
while [ $count -le 10 ]; do
    # Check conditions for different iterations
    if [ $count -eq 4 ]; then
        echo "Bad Luck"
    elif [ $count -eq 8 ]; then
        echo "Good Luck"
    else
        echo "Best School"
    fi

    # Increment the count
    ((count++))
done
